{
  "name": "@jiubao/gallery",
  "version": "0.0.5",
  "description": "photo gallery",
  "main": "dist/gallery.es.js",
  "browser": "dist/gallery.umd.js",
  "module": "dist/gallery.es.js",
  "cjs": "dist/gallery.cjs.js",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rm -rf dist && rollup -c && npm run minify",
    "minify": "uglifyjs $npm_package_browser -cm -o $npm_package_browser",
    "release": "npm run build && npm run minify && standard-version && git push --follow-tags && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiubao/gallery.git"
  },
  "keywords": [
    "photo",
    "gallery"
  ],
  "author": "jiubao <xia.ldj@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jiubao/gallery/issues"
  },
  "homepage": "https://github.com/jiubao/gallery#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "rollup": "^0.66.6",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-embed-css": "^0.3.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "standard-version": "^4.4.0",
    "uglify-js": "^3.4.9"
  },
  "dependencies": {
    "@jiubao/passive": "^0.0.1",
    "@jiubao/raf": "^0.0.2",
    "swipe-core": "^0.1.7"
  }
}
